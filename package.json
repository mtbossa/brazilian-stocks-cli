    "author": "Mateus Ribeiro Bossa <mateus.rbossa@gmail.com>",
    "license": "ISC",
    "version": "0.0.0",
    "keywords": [
        "stocks",
        "strategies",
        "scrapping"
    ],
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "dev": "nodemon --exec ts-node-esm src/index.ts",
