        "eslint": "^8.39.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.0",
        "lint-staged": "^13.2.2",
        "nodemon": "^2.0.22",
        "prettier": "2.8.8",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    },
    "engines": {
        "node": "18.x"
    },
    "dependencies": {
        "prompts": "^2.4.2"
