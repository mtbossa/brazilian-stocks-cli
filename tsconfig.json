{
  "compilerOptions": {
    "target": "ES2022",
    "lib": [
      "ES2022",
    ],
    "module": "ES2022",
    "sourceMap": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
  ],
  "include": [
    "src/**/*.ts"
  ],
}